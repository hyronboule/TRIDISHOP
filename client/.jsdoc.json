{
    "source": {
      "include": ["src/page", "src/components", "src/services"],
      "exclude": ["node_modules", "test"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "templates/default"
    }
  }
  